{
  "name": "mvt-viernes",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "dev": "tsnd ./src/index.ts",
    "test": "jest --verbose",
    "test-c": "jest --coverage --runInBand",
    "test-v": "jest --verbose --runInBand --detectOpenHandles",
    "test-d": "jest --testNamePattern='describe' --verbose",
    "test-f": "jest --testPathPatterns='Environment.test.ts' --verbose",
    "test:e2e": "cross-env NODE_ENV=test jest --config jest.e2e.config.js --runInBand",
    "test:e2e:debug": "cross-env NODE_ENV=test HEADLESS=false jest --config jest.e2e.config.js --runInBand --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.1",
    "@types/puppeteer": "^5.4.7",
    "@types/supertest": "^2.0.16",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "jest-puppeteer": "^11.0.0",
    "puppeteer": "^24.26.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "directories": {
    "test": "test"
  },
  "description": ""
}
